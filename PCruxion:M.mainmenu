local password = "yawaningapassword"

local function activateScript() loadstring(game:HttpGet("https://raw.githubusercontent.com/derockk/Pruject-Cruxion-M/main/PCruxionScript.mobile"))()
end

local function checkPassword(input)
    if input == password then
        activateScript()
    else
        print("Incorrect Password") 
    end
end

