textbox.Size = UDim2.new(0, 480, 0, 50)
textbox.PlaceholderText = "Enter Password. "
textbox.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        checkPassword(textbox.Text)
        textbox:Destroy()
        logoName:Destroy()
windowFrame:Destroy() 
intext1:Destroy() 
    end
end)
